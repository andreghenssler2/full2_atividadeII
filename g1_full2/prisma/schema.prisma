generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Usuario {
  id    Int    @id @default(autoincrement())
  nome  String
  email String @unique
}

model Produto {
  id    Int     @id @default(autoincrement())
  nome  String
  preco Float
}
